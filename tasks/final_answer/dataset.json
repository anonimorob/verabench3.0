{
  "_dataset_metadata": {
    "name": "Vera AI - Response Generation Benchmark Dataset",
    "version": "1.0",
    "created_date": "2025-11-13",
    "description": "30 test cases per valutare response generation agent su faithfulness, answer relevancy e conciseness",
    "total_test_cases": 30,
    
    "test_distribution": {
      "by_category": {
        "permission_responses": 8,
        "data_retrieval_responses": 8,
        "preference_responses": 5,
        "conversation_history_responses": 4,
        "error_responses": 5
      },
      "by_language": {
        "english": 18,
        "italian": 7,
        "arabic": 5
      },
      "by_complexity": {
        "simple": 12,
        "medium": 12,
        "complex": 6
      }
    },
    
    "evaluation_metrics": {
      "faithfulness": "DeepEval - verifica fedeltà ai fatti",
      "answer_relevancy": "DeepEval - verifica pertinenza risposta",
      "conciseness": "Rule-based - max 500 chars, 10 lines"
    }
  },

  "test_cases": [
    {
      "id": "resp_001",
      "category": "permission_responses",
      "user_query": "Can I write to the ERP sales module?",
      "user_preferences": {
        "language": "en",
        "output_format": "concise",
        "timezone": "Asia/Dubai"
      },
      "retrieved_context": {
        "source": "retrieval_agent",
        "data": {
          "can_read": true,
          "can_write": false,
          "modules": ["sales", "inventory", "orders"]
        }
      },
      "expected_response_characteristics": {
        "must_include_concepts": ["read-only", "cannot write", "sales"],
        "must_not_include": ["you can modify", "write access granted"],
        "tone": "informative",
        "max_characters": 500
      },
      "evaluation_config": {
        "faithfulness_threshold": 0.85,
        "answer_relevancy_threshold": 0.85,
        "max_characters": 500,
        "max_lines": 10
      }
    },
