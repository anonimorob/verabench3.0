    {
      "id": "resp_001",
      "category": "permission_responses",
      "complexity": "simple",
      "user_query": "Posso modificare i dati nel modulo vendite ERP?",
      "user_preferences": {
        "language": "it",
        "output_format": "concise",
        "timezone": "Europe/Rome"
      },
      "retrieved_context": {
        "source": "retrieval_agent",
        "data": {
          "can_read": true,
          "can_write": true,
          "modules": [
            "vendite",
            "inventario",
            "ordini"
          ]
        }
      },
      "expected_response_characteristics": {
        "must_include_concepts": [
          "sì",
          "puoi modificare",
          "vendite"
        ],
        "must_not_include": [
          "solo lettura",
          "non puoi"
        ],
        "tone": "affirmative",
        "max_characters": 300
      },
      "evaluation_config": {
        "faithfulness_threshold": 0.9,
        "answer_relevancy_threshold": 0.9,
